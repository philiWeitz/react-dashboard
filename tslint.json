{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template",
      "jsx-double"
    ],
    "no-implicit-dependencies": false,
    "interface-name": false,
    "jsx-boolean-value": false,
    "no-submodule-imports": [true, "@material-ui/core", "react-test-renderer"]
  }
}
